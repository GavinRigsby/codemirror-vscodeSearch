{
  "name": "@rigstech/codemirror-vscodesearch",
  "version": "1.0.4",
  "description": "retexture of codemirror's search to look like VS Code",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GavinRigsby/codemirror-vscodeSearch.git"
  },
  "keywords": [
    "codemirror-vscodeSearch",
    "typescript",
    "codemirror",
    "codemirror-extension"
  ],
  "scripts": {
    "clean": "del-cli dist",
    "build": "tsup",
    "dev": "tsup src/index.ts --watch --onSuccess \"npm start\"",
    "start": "node dist/index.js"
  },
  "author": "Gavin Rigsby <gavinrigsby@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/search": "^6.5.10",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.5"
  },
  "devDependencies": {
    "del-cli": "^6.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "bugs": {
    "url": "https://github.com/GavinRigsby/codemirror-vscodeSearch/issues"
  },
  "homepage": "https://github.com/GavinRigsby/codemirror-vscodeSearch#readme"
}
