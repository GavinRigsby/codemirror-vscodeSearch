{
  "name": "@rigstech/codemirror-vscodeSearch",
  "version": "1.0.5",
  "description": "retexture of codemirror's search to look like VS Code",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GavinRigsby/codemirror-vscodeSearch.git"
  },
  "keywords": ["codemirror-vscodeSearch", "typescript", "codemirror", "codemirror-extension"],
  "scripts": {
    "clean": "del-cli dist",
    "build": "tsup",
    "dev": "tsup src/index.ts --watch --onSuccess \"npm start\"",
    "start": "node dist/index.js"
  },
  "author": {
    "name": "Gavin Rigsby",
    "email": "gavinrigsby@outlook.com"
  },
  "license": "MIT",
  "dependencies": {
    "@codemirror/search":"^6.5.10",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.5",
    "codemirror": "^6.0.1"
  },
  "devDependencies": {
    "del-cli": "^6.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
